<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="500.0" prefWidth="700.0" spacing="60.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.fipp.projetosisdental.RelatoriosViewController">
    <children>
        <TilePane alignment="CENTER" layoutX="60.0" layoutY="231.0" maxWidth="600.0" onMouseClicked="#onRelatorioMateriais" prefHeight="59.0" prefWidth="600.0" style="-fx-background-color: #F5ae42; -fx-background-radius: 10;">
            <children>
                <Label text="MATERIAIS" textFill="WHITE">
                    <font>
                        <Font name="Calibri Bold" size="33.0" />
                    </font>
                </Label>
            </children>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </TilePane>
        <TilePane alignment="CENTER" maxWidth="600.0" onMouseClicked="#onRelatorioProcedimentos" prefHeight="59.0" prefWidth="600.0" style="-fx-background-color: #F5ae42; -fx-background-radius: 10;">
            <children>
                <Label text="PROCEDIMENTOS" textFill="WHITE">
                    <font>
                        <Font name="Calibri Bold" size="33.0" />
                    </font>
                </Label>
            </children>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </TilePane>
      <TilePane alignment="CENTER" layoutX="60.0" layoutY="290.0" maxWidth="600.0" onMouseClicked="#onRelatorioPacientes" prefHeight="59.0" prefWidth="600.0" style="-fx-background-color: #F5ae42; -fx-background-radius: 10;">
         <children>
            <Label text="PACIENTES" textFill="WHITE">
               <font>
                  <Font name="Calibri Bold" size="33.0" />
               </font>
            </Label>
         </children>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </TilePane>
      <TilePane alignment="CENTER" layoutX="60.0" layoutY="112.0" maxWidth="600.0" onMouseClicked="#onRelatorioAgenda" prefHeight="59.0" prefWidth="600.0" style="-fx-background-color: #F5ae42; -fx-background-radius: 10;">
         <children>
            <Label text="AGENDA DO DIA" textFill="WHITE">
               <font>
                  <Font name="Calibri Bold" size="33.0" />
               </font>
            </Label>
         </children>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </TilePane>
    </children>
</VBox>
